#include<stdio.h>
int main()
{
    int a[10],b[10],c,d;
    printf("Enter size of set A\n");
    scanf("%d",&c);
    printf("Enter element of set A\n");
    for(int i=0;i<c;i++)
      scanf("%d",&a[i]);
    printf("Enter size of set B\n");
    scanf("%d",&d);
    printf("Enter element of set B\n");
    for(int i=0;i<d;i++)
    scanf("%d",&b[i]);
    printf("{");
    for(int i=0;i<c;i++)
    {
        for(int j=0;j<d;j++)
        {
            printf(" (%d %d) ",a[i],b[j]);
        }
    }
    printf("}");
    return 0;
}
